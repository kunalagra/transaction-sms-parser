{
  "name": "transaction-sms-parser",
  "version": "3.0.0",
  "description": "A library to parse transaction sms text",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/saurabhgupta050890/transaction-sms-parser.git"
  },
  "type": "module",
  "main": "./dist/lib.umd.cjs",
  "module": "./dist/lib.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/lib.js",
      "require": "./dist/lib.umd.cjs"
    }
  },
  "files": [
    "dist",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "license": "MIT",
  "keywords": [
    "parser",
    "typescript",
    "sms",
    "finance",
    "personal finance",
    "transaction parser",
    "bank sms"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "run-s test:pre test:unit",
    "test:unit": "vitest run",
    "test:ui": "vitest ui",
    "test:pre": "run-s test:lint test:prettier test:spelling",
    "test:lint": "eslint src --ext .ts",
    "test:prettier": "prettier \"src/**/*.ts\" --list-different",
    "test:spelling": "cspell \"{README.md,.github/*.md,docs/*.md}\"",
    "fix": "run-s fix:*",
    "fix:prettier": "prettier \"src/**/*.ts\" --write",
    "fix:lint": "eslint src --ext .ts --fix",
    "release": "standard-version"
  },
  "devDependencies": {
    "@types/convert-excel-to-json": "^1.7.4",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "convert-excel-to-json": "^1.7.0",
    "cspell": "^8.0.0",
    "csvtojson": "^2.0.10",
    "eslint": "^8.54.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-prettier": "^5.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.0",
    "standard-version": "^9.5.0",
    "typescript": "^5.3.2",
    "vite": "^5.0.2",
    "vite-plugin-dts": "^3.6.3",
    "vitest": "^0.34.6"
  }
}